  // NEWSLETTER

  "flex-layout.row#newsletter-footer": {
    "title": "Bloco newsletrer home",
    "props": {
      "blockClass": "newsletter-footer",
      "fullWidth": false,
      "marginBottom": 9,
      "marginTop": 8
    },
    "children": [
      "flex-layout.col#newsletter-footer-left",
      "flex-layout.col#newsletter-footer-rigth"
    ]
  },

  "flex-layout.col#newsletter-footer-left":{
    "children": [ "rich-text#newsletter-footer-title"],
    "props": {
      "width": "40%"
    }
  },

  "rich-text#newsletter-footer-title": {
    "props": {
      "text": "Cadastre-se em nossa \n ### NEWSLETTER \n e fique por dentro de ofertas e novidades",
      "blockClass": "newsletter-footer",
      "verticalAlign": "middle"
    }
  },

  "flex-layout.col#newsletter-footer-rigth":{
    "children": ["newsletter-footer"],
    "props": {
      "width": "60%",
      "verticalAlign": "middle"
    }
